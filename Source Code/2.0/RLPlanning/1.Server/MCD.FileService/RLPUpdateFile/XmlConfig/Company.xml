<?xml version="1.0" encoding="utf-8" ?>
<SqlMapConfig >
  <!-- 数据库链接字符串名 -->
  <Provider name="DBConnection"></Provider>
  <Statements>
    <Procedure id="SelectSingle" value="SRLS_USP_Master_SelectSingleCompany">
      <Parameters>
        <Parameter name="@CompanyCode" type="nvarchar(32)" direction="in"/>
      </Parameters>
    </Procedure>
    <!-- 更新公司信息存储过程 -->
    <Procedure id="UpdateEntity"  value="SRLS_USP_Master_UpdateSingleCompany">
      <Parameters>
        <Parameter name="@CompanyCode" type="varchar(32)" direction="in"/>
        <Parameter name="@SimpleName" type="varchar(32)" direction="in"/>
        <Parameter name="@FixedSourceCode" type="varchar(50)" direction="in"/>
        <Parameter name="@FixedManageSourceCode" type="varchar(50)" direction="in"/>
        <Parameter name="@FixedServiceSourceCode" type="varchar(50)" direction="in"/>
        <Parameter name="@StraightLineSourceCode" type="varchar(50)" direction="in"/>
        <Parameter name="@RatioSourceCode" type="varchar(50)" direction="in"/>
        <Parameter name="@RatioManageSourceCode" type="varchar(50)" direction="in"/>
        <Parameter name="@RatioServiceSourceCode" type="varchar(50)" direction="in"/>
        <Parameter name="@ResponsibleUserID" type="varchar(50)" direction="in"/>
        <Parameter name="@ResponsibleEnglishName" type="varchar(50)" direction="in"/>
        <Parameter name="@OperationID" type="uniqueidentifier" direction="in"/>
        <Parameter name="@UpdateTime" type="datetime" direction="in"/>
      </Parameters>
    </Procedure>
    <Procedure id="SelectTable"  value="SRLS_USP_Master_SelectCompany">
      <Parameters>
        <Parameter name="@CompanyName" type="nvarchar(32)" description="公司名称"/>
        <Parameter name="@CompanyCode" type="nvarchar(32)" description="公司编码"/>
        <Parameter name="@Status" type="nvarchar(8)" description="状态"/>
      </Parameters>
    </Procedure>
  </Statements>
</SqlMapConfig>